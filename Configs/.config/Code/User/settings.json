{
	// Default
	"telemetry.telemetryLevel": "off",
	"workbench.colorTheme": "Catppuccin Mocha",
	"catppuccin.workbenchMode": "flat",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.sideBar.location": "right",
	"window.titleBarStyle": "custom",
	//"workbench.activityBar.location": "top",
	// "workbench.startupEditor": "none",
	"window.zoomLevel": 1,
	"window.commandCenter": false,
	"workbench.tree.indent": 4,
	"files.eol": "\n",
	//? editor
	"editor.fontLigatures": true,
	"editor.multiDocumentOccurrencesHighlight": true,
	"editor.fontSize": 14,
	"editor.minimap.side": "right",
	"editor.minimap.size": "fit",
	"editor.fontFamily": "'JetBrainsMono Nerd Font', 'CaskaydiaCove Nerd Font Mono', 'Arimo Nerd Font', 'MesloLGS Nerd Font', 'monospace', monospace",
	"editor.wordWrap": "on",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.mouseWheelZoom": true,
	"editor.linkedEditing": true,
	"editor.formatOnType": true,
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"editor.stickyScroll.enabled": true,
	"editor.minimap.showSlider": "always",
	"editor.unicodeHighlight.includeComments": false,
	//? git
	"git.autofetch": true,
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"git.allowForcePush": true,
	"git.showPushSuccessNotification": true,
	"git.openRepositoryInParentFolders": "always",
	//? gitlens
	"gitlens.views.repositories.files.layout": "list",
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.rebaseEditor.ordering": "asc",
	"gitlens.graph.dimMergeCommits": true,
	"gitlens.graph.showRemoteNames": true,
	"gitlens.views.commits.pullRequests.showForBranches": false,
	"gitlens.views.commits.pullRequests.enabled": false,
	"gitlens.codeLens.scopes": [
		"document",
		"containers"
	],
	"gitlens.graph.minimap.additionalTypes": [
		"localBranches",
		"stashes",
		"remoteBranches",
		"tags"
	],
	//? terminal (Linux)
	"terminal.external.linuxExec": "kitty",
	"terminal.sourceControlRepositoriesKind": "external",
	"terminal.explorerKind": "external",
	"terminal.integrated.smoothScrolling": true,
	"terminal.integrated.sendKeybindingsToShell": true,
	"terminal.integrated.defaultProfile.linux": "zsh",
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "bash",
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh",
			"icon": "terminal-linux"
		},
		"fish": {
			"path": "fish",
			"icon": "terminal-linux"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	//? terminal (Windows)
	"terminal.integrated.enableBell": true,
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"terminal.integrated.profiles.windows": {
		"PowerShell": {
			"source": "PowerShell",
			"icon": "terminal-powershell"
		},
		"Command Prompt": {
			"path": [
				"${env:windir}\\Sysnative\\cmd.exe",
				"${env:windir}\\System32\\cmd.exe"
			],
			"args": [],
			"icon": "terminal-cmd"
		},
		"Git Bash": {
			"source": "Git Bash"
		},
	},
	"security.workspace.trust.untrustedFiles": "newWindow",
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.enabled": false,
	"extensions.autoUpdate": false,
	"better-comments.highlightPlainText": true,
	"better-comments.tags": [
		{
			"tag": "!",
			"color": "#CC826A",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#E9967A10",
			"bold": false,
			"italic": false
		},
		{
			"tag": "?",
			"color": "#B2A056",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#FFE26E10",
			"bold": false,
			"italic": true
		},
		{
			"tag": "//",
			"color": "#4C4C4C",
			"strikethrough": true,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "todo",
			"color": "#CC7000",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#FF8C0010",
			"bold": false,
			"italic": false
		},
		{
			"tag": "*",
			"color": "#718066",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#98C37920",
			"bold": false,
			"italic": false
		},
		{
			"tag": "@",
			"color": "#63A9E6",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "transparent",
			"bold": false,
			"italic": false
		},
		{
			"tag": "$",
			"color": "#CC828E",
			"strikethrough": false,
			"underline": false,
			"backgroundColor": "#ff002b10",
			"bold": false,
			"italic": false
		}
	],
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	"liveServer.settings.donotVerifyTags": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.AdvanceCustomBrowserCmdLine": "",
	"liveServer.settings.fullReload": true,
	"vscord.status.image.small.notInFile.text": "Snoozing zZz",
	"vscord.status.buttons.button1.active.label": "Check this out? üëá",
	"vscord.status.buttons.button1.idle.enabled": true,
	"vscord.status.buttons.button1.idle.label": "Wait, what's this? üëáü§î",
	"vscord.status.details.text.idle": "random üë®‚Äçüíª",
	"vscord.status.details.text.notInFile": "just code!",
	"vscord.status.details.text.viewing": "",
	"vscord.status.buttons.button1.active.enabled": true,
	"vscord.status.buttons.button1.git.active.label": "Check Repo üëá",
	"vscord.status.details.text.editing": "",
	"vscord.behaviour.suppressNotifications": true,
	"htmlhint.documentSelector": [
		"html",
		"htm"
	],
	"html.format.indentInnerHtml": true,
	"html.format.wrapAttributes": "preserve-aligned",
	"html.format.indentHandlebars": true,
	"html.format.wrapLineLength": 0,
	"less.format.spaceAroundSelectorSeparator": true,
	"css.format.spaceAroundSelectorSeparator": true,
	"scss.format.spaceAroundSelectorSeparator": true,
	"eslint.format.enable": true,
	"autoprefixer.formatOnSave": true,
	"bookmarks.useWorkaroundForFormatters": true,
	"prettier.useTabs": true,
	"prettier.endOfLine": "lf",
	"prettier.bracketSameLine": true,
	"prettier.printWidth": 100,
	"prettier.insertPragma": true,
	"prettier.requirePragma": true,
	"[html]": {
		"editor.tabSize": 3,
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
		"workbench.editor.languageDetection": false
	},
	"scss.lint.float": "warning",
	"[scss]": {
		"editor.defaultFormatter": "vscode.css-language-features"
	},
	"[css]": {},
	"[javascript]": {},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"vs-code-prettier-eslint.prettierLast": true,
	"vscode-pets.petColor": "white",
	"vscode-pets.petSize": "large",
	"vscode-pets.throwBallWithMouse": true,
	"vscode-pets.petType": "totoro",
	"svg.preview.transparencyGrid": false,
	"svg.preview.boundingBox": false,
	"indentRainbow.lightIndicatorStyleLineWidth": 12,
	"hungryDelete.keepOneSpace": true,
	"todo-tree.highlights.enabled": false,
	"indentRainbow.indicatorStyle": "light",
	"hungryDelete.languageConfigurations": [
		{
			"languageId": "html",
			"indentationRules": {
				"increaseIndentPattern": "<(?!\\?|(?:area|base|br|col|frame|hr|html|img|input|link|meta|param)\\b|[^>]*\\/>)([-_\\.A-Za-z0-9]+)(?=\\s|>)\\b[^>]*>(?!.*<\\/\\1>)|<!--(?!.*-->)|\\{[^}\"']*$",
				"decreaseIndentPattern": "^\\s*(<\\/(?!html)[-_\\.A-Za-z0-9]+\\b[^>]*>|-->|\\})"
			}
		},
		{
			"languageId": "scss",
			"indentationRules": {
				"increaseIndentPattern": "(^.*\\{[^}]*$)",
				"decreaseIndentPattern": "^\\s*\\}"
			}
		}
	],
	"vscode_vibrancy.opacity": 0.95,
	"scss.format.maxPreserveNewLines": 100,
	"codesnap.backgroundColor": "hsla(202, 46%, 72%, 1)",
	"codesnap.containerPadding": "2.5em",
	"fiveServer.injectBody": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"name": "One Dark italic",
				"scope": [
					"comment",
					"entity.other.attribute-name",
					"keyword",
					"markup.underline.link",
					"storage.modifier",
					"storage.type",
					"string.url",
					"variable.language.super",
					"variable.language.this"
				],
				"settings": {
					"fontStyle": "italic"
				}
			},
			{
				"name": "One Dark italic reset",
				"scope": [
					"keyword.operator",
					"keyword.other.type",
					"storage.modifier.import",
					"storage.modifier.package",
					"storage.type.built-in",
					"storage.type.function.arrow",
					"storage.type.generic",
					"storage.type.java",
					"storage.type.primitive"
				],
				"settings": {
					"fontStyle": ""
				}
			}
		]
	},
	"console-ninja.fontSize": 14,
	"console-ninja.outputMode": "In View",
	"console-ninja.captureFunctions": true,
	"console-ninja.toolsToEnableSupportAutomaticallyFor": {
		"live-server-extension": true
	},
	//? Animations
	"apc.stylesheet": {
		".monaco-editor .cursor": "background: #ffffffaa !important; box-shadow: 0 0 70px 5px #ffffff, #ffffff 0px 0px 34px 1px; color: [#161616](https://github.com/AlexOwl/vscode-transparent-color-theme/issues/161616) !important",
	},
	"animations.Enabled": false,
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": "on",
	"animations.Active": "Indent",
	"animations.Scrolling": "None",
	"animations.Tabs": "Slide",
	"vsicons.dontShowNewVersionMessage": true,
	"glassit-linux.opacity": 98,
	"console-ninja.featureSet": "Community",
	"animations.Install-Method": "Apc Customize UI++",
	"apc.imports": [
		"file:///home/devck/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.1/dist/updateHandler.js"
	],
	// end of animations
	"markdown.editor.pasteUrlAsFormattedLink.enabled": "smart",
	"markdownlint.config": {
		"MD033": false // allow inline HTML
	},
	"diffEditor.wordWrap": "on",
	"diffEditor.experimental.showMoves": true,
	"git-autoconfig.configList": [
		{
			"user.email": "vargaschrisking@gmail.com",
			"user.name": "devCKVargas"
		},
		{}
	],
	"license.author": "Chris King Vargas",
	"license.year": "auto",
	"license.default": "gpl-3.0",
	"license.filename": "LICENSE",
	"license.extension": "",
	"emojisense.languages": {
		"markdown": true,
		"plaintext": {
			"markupCompletionsEnabled": false,
			"emojiDecoratorsEnabled": false
		},
		"scminput": true,
		"git-commit": true
	},
}